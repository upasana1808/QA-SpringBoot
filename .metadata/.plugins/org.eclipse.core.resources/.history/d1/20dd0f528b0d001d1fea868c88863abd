package com.example.model;

public class Book {
	int id;
	String title;
	String price;
	String author;
	public Book(int id, String title, String price, String author) {
		super();
		this.id = id;
		this.title = title;
		this.price = price;
		this.author = author;
	}
	public Book() {
		super();
	}
	
	
	

}
